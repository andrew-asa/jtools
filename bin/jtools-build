#!/bin/sh
JAR_DIR="/Users/andrew_asa/Documents/code/github/andrew-asa/exec/jtools/"
cd $JAR_DIR
if [ $# -eq 1 ] ; then
    if [ "$1" = "dep" ];then
        SHELL="mvn dependency:copy-dependencies -DoutputDirectory=./lib"
        echo $SHELL
        $SHELL
    elif [ "$1" = "jar" ];then
        SHELL="mvn jar:jar"
        echo $SHELL
        $SHELL
    fi
    exit 1;
fi


