#!/bin/sh
JAR_DIR="/Users/andrew_asa/Documents/code/github/andrew-asa/exec/jtools"
cd $JAR_DIR
JAR="com.asa.jtools-1.0-SNAPSHOT.jar"
JAR_DIR="$JAR_DIR/target/"
CLASS_PACKAGE="com.asa.jtools.bin"
DEFAULT_CMD="Ls"
if [ $# -lt 1 ] ; then
echo "输入命令名称"
#exit 1;
CMD=$DEFAULT_CMD
else
CMD=$1
fi
ARG=""
for((i=2;i<=$#;i++));
do
ARG="${ARG} ${!i} "
done
#echo $CMD $ARG
SHELL="java -cp "$JAR_DIR$JAR:lib/*" "$CLASS_PACKAGE.$CMD" "$ARG""
echo $SHELL
$SHELL

#java -cp $(echo lib/*.jar | tr ' ' ':') "com.asa.jtools.bin/SwitchHosts"
#java -cp "lib/*" "com.asa.jtools.bin/SwitchHosts"
#java -cp "target/com.asa.jtools-1.0-SNAPSHOT.jar:lib/*" "com.asa.jtools.bin/SwitchHosts"

